Private Const c_strTitle = "Примеры текстовых функций"
Private Const c_strComment = "Кнопка ""Волшебная палочка"" в формах примеров меняет образцы выражений." & vbCrLf & _
                             "Все используемые в примерах функции и комментарии к ним содержатся в модуле modStrings."
Private Const c_strLink = ""

Private Const c_lngCtlGap = 0.05 * cm
Private Const Offset = 2
Private Const c_RowTitleHeight = 1      ' множитель (количество строк)
Private Const c_RowCommentHeight = 1    ' множитель (количество строк)
Private iMin As Byte, iMax As Byte, iStep As Byte

Private Function Samples()
' массив данных для примера
' Элемент массива: [i+0]=Index, [i+1]=Title, [i+2]=Comment, [i+3]=FormName
On Error Resume Next
Static arrData(): iMin = LBound(arrData): If Err Then Err.Clear Else Samples = arrData: Exit Function
    arrData = Array(1, "Расстановка переносов в поле.", "Функции: HyphenateWord и TextToArrayByWidth", "HyphenateWords", _
                    2, "Расстановка переносов с распределением по полям.", "Функции: HyphenateWord и TextToArrayByControl", "HyphenateWords2", _
                    3, "Склонение слов по падежам.", "Функции: DeclineWords и DeclineWord", "DeclineWords", _
                    4, "Преобразование числа в текст и склонение по падежам.", "Функции: NumToWords и DeclineWord", "DeclineNumbers", _
                    5, "Сравнение слов.", "Функции: Levenshtein, MetaphoneRu, Soundex, etc.", "CompareWords", _
                    6, "Преобразование альфа символов.", "Функции:  TextAlpha2Code, TextCode2Alpha и GenPassword", "AlphaText", _
                    7, "Работа со строками с разделителями.", "Функции:  DelimString[Get|Set|Del], TaggedString[Get|Set|Del], TokenString[Get|Set|Del]", "DelimitedText", _
                    8, "Работа с подстановочными выражениями.", "Функции:  PlaceHoldersSet, TaggedString2Collection", "PlaceHolders", _
                    9, "Разбор скобок в выражениях.", "Функции:  GroupsGet", "Brackets" _
        )
    iStep = 4: iMin = LBound(arrData): iMax = UBound(arrData)
HandleExit: Samples = arrData
End Function

Private Sub Form_Open(Cancel As Integer)
    AccWinHide
Const cShadow = 0.075 * cm
Dim i As Byte
Dim lRowPos As Long, lCtlPos As Long, lRowHeight As Long
Dim strNum As String * 3
    lblTitle.Caption = c_strTitle: lblTitle2.Caption = lblTitle.Caption: lblTitle2.Left = lblTitle.Left + cShadow: lblTitle2.Top = lblTitle.Top + cShadow
    lblComment.Caption = c_strComment
    lblLink.Caption = c_strLink: lblLink.HyperlinkAddress = c_strLink
    lRowHeight = c_RowTitleHeight * txt001_Title.Height + c_RowCommentHeight * txt001_Comment.Height + c_lngCtlGap
    lRowPos = 0
' инициализация данных статического массива
    iMin = LBound(Samples)
' подготовка формы
    For i = iMin To iMax Step iStep
        Section(acDetail).Height = lRowPos + lRowHeight
        strNum = Format$(Samples(i), "000")
        With Controls("cmd" & strNum): lCtlPos = lRowPos + (lRowHeight - .Height) \ 2: .Top = lCtlPos: .Caption = Samples(i): .OnClick = "=OpenForm(""" & Samples(i + 3) & """)": .Visible = True: End With
        With Controls("txt" & strNum & "_Title"): lCtlPos = lRowPos: .Top = lRowPos: .Height = .Height * c_RowTitleHeight: .Caption = Samples(i + 1): .Visible = True: lCtlPos = lCtlPos + .Height: End With
        With Controls("txt" & strNum & "_Comment"): .Top = lCtlPos: .Height = .Height * c_RowCommentHeight: .Caption = Samples(i + 2): .Visible = True: lCtlPos = lCtlPos + .Height: End With
        lRowPos = lCtlPos + c_lngCtlGap
    Next i
    InsideHeight = Section(acDetail).Height + Section(acHeader).Height + Section(acFooter).Height
' форматирование формы
    Call AccObjectSet(Me) ', Arrange:=eCenter)
End Sub

Private Sub Form_Close(): AccWinShow: End Sub

Private Function OpenForm(FormName As String) As Boolean
Const c_strProcedure = "OpenForm"
Dim Result As Boolean: Result = False
    On Error GoTo HandleError
    'DoCmd.OpenForm FormName
    Result = FormOpen(FormName, PARENT:=ActiveControl, Placement:=ePlaceOnRightToTop)
HandleExit:  OpenForm = Result: Exit Function
HandleError: Result = False: Err.Clear: Resume HandleExit
End Function