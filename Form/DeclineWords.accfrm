Private Function p_SwitchSamples(ByRef i As Byte) As Byte
    If i < iMin Or i > iMax Then i = iMin
    txtInput.Value = Samples(i)
    chkAnimate = Samples(i + 1)
    chkIsFIO = Samples(i + 2)
    txtSkipWords = Samples(i + 3)
    p_SwitchSamples = i
End Function