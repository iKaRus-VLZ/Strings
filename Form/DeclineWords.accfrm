CodeBehindForm
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = True
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Compare Database
Option Explicit
Private Const c_strTitle = "Склонение слов"
Private Const c_strComment = "Основное назначение - склонение пар ""подлежащее - определение"" и ФИО." & vbCrLf & _
                             "Пример не делает разбор предложения, не анализирует семантику и не занимается согласованием слов в фразе, также - никак не различает одушевленные/неодушевленные. Просто бьет строку на куски по набору разделителей и склоняет каждое слово, кроме указанных в списке пропуска, по набору правил/исключений заданных в функции DeclineWord." & vbCrLf & _
                             "На самом деле этого вполне достаточно в 90% случаев. Если что-то склоняется неправильно, единственный выход - скорректировать правила под себя."
Private Const c_strLink = ""
Private Const Indent = 70 '0.125 * cm   ' отступ от края формы и расстояние между контроламиPrivate Const Indent = 0.15 * cm
Private iMin As Byte, iMax As Byte, iStep As Byte
Private m_bytState As Byte
Private Sub Form_Open(Cancel As Integer):
Const cShadow = 0.075 * cm
    lblTitle.Caption = c_strTitle: lblTitle2.Caption = lblTitle.Caption: lblTitle2.Left = lblTitle.Left + cShadow: lblTitle2.Top = lblTitle.Top + cShadow
    lblComment.Caption = c_strComment
    'lblLink.Caption = c_strLink: lblLink.HyperlinkAddress = c_strLink
    InsideHeight = Section(acDetail).Height + Section(acHeader).Height + Section(acFooter).Height
' начальные значения
    iMin = LBound(Samples): iMax = UBound(Samples): iStep = 4
    m_bytState = p_SwitchSamples(iMin)
Dim i As Byte
    ' =DeclineWords(Words,NewCase,NewNumb,NewGend,Animate,IsFio,SkipWords)
Dim aDec: aDec = Array(DeclineCaseImen, DeclineCaseRod, DeclineCaseDat, DeclineCaseVin, DeclineCaseTvor, DeclineCasePred)
Dim aTxt: aTxt = Array(DeclineCaseImen & ": им. (кто, что?)", _
                       DeclineCaseRod & ": род. (кого, чего?)", _
                       DeclineCaseDat & ": дат. (кому, чему?)", _
                       DeclineCaseVin & ": вин. (кого, что?)", _
                       DeclineCaseTvor & ": твор. (кем, чем?)", _
                       DeclineCasePred & ": пред. (о ком, о чём?)")
    For i = 0 To 5
        With Me.Controls("txt" & i)
            .ControlSource = "=DeclineWords(Nz([txtInput],'')," & aDec(i) & ",txtNewNumb,txtWordGender,chkAnimate,chkIsFIO,Nz(txtSkipWords,''))":
            .Controls(0).Caption = aTxt(i)
        End With
    Next i
    Form_Resize
End Sub
Private Sub Form_Resize()
' обрабатываем изменение границ
    On Error Resume Next 'GoTo HandleError
Dim tmpWidth As Long:   tmpWidth = Me.InsideWidth '- ConvPixelsToTwips(rct.Right - rct.Left)
Dim i As Long
    For i = 1 To 9
        With Controls("Line" & Format$(i, "00"))
            If Err Then Err.Clear Else .Left = Indent: .Width = tmpWidth - 2 * Indent
        End With
    Next i
' ресайзим дополнительные контролы по форме
    With lblTitle:      .Width = tmpWidth - 4 * Indent: End With
    With lblTitle2:     .Width = tmpWidth - 4 * Indent: End With
    With lblComment:    .Width = tmpWidth - 4 * Indent: End With
    'With lblLink:       .Width = tmpWidth - 4 * Indent: End With
    
    On Error GoTo HandleError
    On Error GoTo HandleError
    With cmdNew
        .Left = InsideWidth - .Width - Indent
    End With
    With txtInput
        .Width = InsideWidth - 4 * cm - cmdNew.Width - 3 * Indent
        .Left = Indent + 4 * cm ': .Top = Indent
    End With
    For i = 0 To 5
    With Controls("txt" & i)
        .Width = InsideWidth - 4 * cm - 2 * Indent
        .Left = Indent + 4 * cm ': .Top = Indent
    End With
    Next i

HandleExit:  Exit Sub
HandleError: Err.Clear: Resume HandleExit
    Select Case Err.Number
    Case 2100 ' Элемент не может быть размещен в указанном месте
'        Section(acDetail).Height = Section(acDetail).Height + 2 * Indent
    Case Else: Stop
    End Select
    Err.Clear: Resume HandleExit
End Sub

