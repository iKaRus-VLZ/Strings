Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = True
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Compare Database
Option Explicit
Private Const c_strTitle = "Преобразование числа в текст"
Private Const c_strComment = "Основное назначение - преобразование числа в текст и склонение его по падежам." & vbCrLf & _
                             "Допустимы целые числа, десятичные дроби (не в экспоненциальном виде), и натуральные дроби. Максимальное известное функции обозначение чисел - дециллион (10^33)." & vbCrLf & _
                             "Контроль соответствия размерности обозначения дробной части (копеек,..) знаменателю дроби не производится поэтому обозначение дополнительной единицы измерения  и её разрядность в десятичной дроби должны соответствовать друг другу,  т.е. если Unit = ""рубль"" и SubUnit = ""копейка"",  DecimalPlaces д.б. = 2 (1/100 руб.), а если Unit = ""килограмм"" и SubUnit = ""грамм"", DecimalPlaces д.б. = 3 (1/1000 кг). Если DecimalPlaces не задан - все варианты типа: ""0,1"";""0,01"" и ""0,001"" ""рубль""/""копейка"" будут выведены как ""ноль рублей одна копейка""."
Private Const c_strLink = ""
Private Const Indent = 70 '0.125 * cm   ' отступ от края формы и расстояние между контролами
Private iMin As Byte, iMax As Byte, iStep As Byte
Private m_bytState As Byte
Private Sub Form_Open(Cancel As Integer):
Const cShadow = 0.075 * cm
    lblTitle.Caption = c_strTitle: lblTitle2.Caption = lblTitle.Caption: lblTitle2.Left = lblTitle.Left + cShadow: lblTitle2.Top = lblTitle.Top + cShadow
    lblComment.Caption = c_strComment
    'lblLink.Caption = c_strLink: lblLink.HyperlinkAddress = c_strLink
    InsideHeight = Section(acDetail).Height + Section(acHeader).Height + Section(acFooter).Height
' начальные значения
    iMin = LBound(Samples): iMax = UBound(Samples): iStep = 4
    m_bytState = p_SwitchSamples(iMin)
Dim i As Byte
Dim aDec: aDec = Array(DeclineCaseImen, DeclineCaseRod, DeclineCaseDat, DeclineCaseVin, DeclineCaseTvor, DeclineCasePred)
Dim aTxt: aTxt = Array(DeclineCaseImen & ": им. (кто, что?)", _
                       DeclineCaseRod & ": род. (кого, чего?)", _
                       DeclineCaseDat & ": дат. (кому, чему?)", _
                       DeclineCaseVin & ": вин. (кого, что?)", _
                       DeclineCaseTvor & ": твор. (кем, чем?)", _
                       DeclineCasePred & ": пред. (о ком, о чём?)")
    For i = 0 To 5
        With Me.Controls("txt" & i)
            .ControlSource = "=NumToWords(Nz([txtInput],'')," & aDec(i) & ",0,0,[chkAnimate],Nz([txtNumbType],1),Nz([txtUnit],''),Nz([txtSubUnit],''),Nz([txtDecimalPlaces],0),[chkTransFrac])"
            .Controls(0).Caption = aTxt(i)
        End With
    Next i
    Form_Resize
End Sub
Private Sub Form_Resize()
' обрабатываем изменение границ
    On Error Resume Next 'GoTo HandleError
Dim tmpWidth As Long:   tmpWidth = Me.InsideWidth '- ConvPixelsToTwips(rct.Right - rct.Left)
Dim i As Long
    For i = 1 To 9
        With Controls("Line" & Format$(i, "00"))
            If Err Then Err.Clear Else .Left = Indent: .Width = tmpWidth - 2 * Indent
        End With
    Next i
' ресайзим дополнительные контролы по форме
    With lblTitle:      .Width = tmpWidth - 4 * Indent: End With
    With lblTitle2:     .Width = tmpWidth - 4 * Indent: End With
    With lblComment:    .Width = tmpWidth - 4 * Indent: End With
    'With lblLink:       .Width = tmpWidth - 4 * Indent: End With
    
    On Error GoTo HandleError
    With cmdNew
        .Left = InsideWidth - .Width - Indent
    End With
    With txtInput
        .Width = InsideWidth - 4 * cm - cmdNew.Width - 3 * Indent
        .Left = Indent + 4 * cm ': .Top = Indent
    End With
    With txtWholeDig.Controls(0): .Left = cmdNew.Left - .Width - Indent: End With
    With txtWholeDig: .Left = .Controls(0).Left - .Width - Indent: End With
    With lblWholeDig: .Left = txtWholeDig.Left - .Width - Indent: End With
    With txtNominDig.Controls(0): .Left = cmdNew.Left - .Width - Indent: End With
    With txtNominDig: .Left = .Controls(0).Left - .Width - Indent: End With
    With lblNominDig: .Left = txtNominDig.Left - .Width - Indent: End With
    With txtDenomDig.Controls(0): .Left = cmdNew.Left - .Width - Indent: End With
    With txtDenomDig: .Left = .Controls(0).Left - .Width - Indent: End With
    With lblDenomDig: .Left = txtDenomDig.Left - .Width - Indent: End With
    With txtFracType.Controls(0): .Left = lblWholeDig.Left - .Width - Indent: End With
    With txtFracType: .Left = lblWholeDig.Left - .Width - Indent: End With
    With chkTransFrac: .Left = txtFracType.Left: .Controls(0).Left = .Left - .Controls(0).Width - Indent: End With
    
    For i = 0 To 5
    With Controls("txt" & i)
        .Width = InsideWidth - 4 * cm - 2 * Indent
        .Left = Indent + 4 * cm ': .Top = Indent
    End With
    Next i

HandleExit:  Exit Sub
HandleError: Err.Clear: Resume HandleExit
    Select Case Err.Number
    Case 2100 ' Элемент не может быть размещен в указанном месте
'        Section(acDetail).Height = Section(acDetail).Height + 2 * Indent
    Case Else: Stop
    End Select
    Err.Clear: Resume HandleExit
End Sub
Private Sub txtWholeDig_AfterUpdate(): txtInput = p_GetNumber(): End Sub
Private Sub txtNominDig_AfterUpdate(): txtInput = p_GetNumber(): End Sub
Private Sub txtDenomDig_AfterUpdate(): txtInput = p_GetNumber(): End Sub
Private Sub txtNumbType_AfterUpdate(): txtInput = p_GetNumber(): End Sub
Private Sub txtDecimalPlaces_AfterUpdate(): txtInput = p_GetNumber(): End Sub
Private Sub txtFracType_AfterUpdate(): p_GenParams: txtInput = p_GetNumber(): End Sub
Private Sub cmdNew_Click(): m_bytState = p_SwitchSamples(m_bytState + iStep): End Sub
Private Function Samples()
' массив данных для примера
' Элемент массива: [i+0]=Unit, [i+1]=SubUnit, [i+2]=DecimalPlaces, [i+3]=NumbType
    Samples = Array( _
        "рубль", "копейка", 2, 1, _
        "килограмм", "грамм", 3, 1, _
        "верста", "сажень", 3, 1, _
        "место", "", 0, 1, _
        "мешок", "", 0, 1, _
        "номер", "", 0, 2, _
        "вопрос", "", 0, 2 _
        )
End Function
Private Function p_SwitchSamples(ByRef i As Byte) As Byte
    If i < iMin Or i > iMax Then i = iMin
    txtUnit = Samples(i)
    txtSubUnit = Samples(i + 1)
    txtDecimalPlaces = Samples(i + 2)
    txtNumbType = Samples(i + 3)
' формируем число
    txtFracType = CInt(Rnd() * 2)       ' определяем тип числа целое/дес.дробь/нат.дробь
    Call p_GenParams                    ' генерируем параметры числа
    txtInput = p_GetNumber()            ' генерируем число указанного типа
    p_SwitchSamples = i
End Function
Private Function p_GenParams()
    txtWholeDig = CInt(Rnd() * 10) + 1  ' определяем число разрядов в целой части
    txtNominDig = CInt(Rnd() * 3) + 1   ' определяем число разрядов в числителе
    txtDenomDig = CInt(Rnd() * 3) + 1   ' определяем число разрядов в знаменателе
End Function
Private Function p_GetNumber() As String
Dim lType As Byte
Dim lWhole As Byte, lNomin As Byte, lDenom As Byte
Dim lPlaces As Byte, bHasSubUnit As Boolean
Dim Result As String
On Error Resume Next
    lType = txtFracType.Value ': If lType = 0 Then lNomin = 0: lDenom = 0
    lWhole = Nz(txtWholeDig.Value, 0)
    lNomin = Nz(txtNominDig.Value, 0): If lNomin = 0 Then lType = 0
    lDenom = Nz(txtDenomDig.Value, 0): If lNomin = 0 Then lDenom = 0
    lPlaces = Nz(txtDecimalPlaces.Value, 0)
    bHasSubUnit = Len(Nz(txtSubUnit, vbNullString)) > 0
    
    Result = Format$(p_GenRandom(lWhole), "#,##0")  ' целая часть
    Select Case lType
    Case 0 ' целое число
        lNomin = 0: lDenom = 0
    Case 1 ' десятичная дробь
        lDenom = 0
        If (lNomin > lPlaces) And bHasSubUnit Then lNomin = lPlaces    ' размерность вспомогательной единицы должна соблюдаться
        If Not bHasSubUnit Then lPlaces = lNomin
        If (lPlaces > 0) Then Result = Result & "," & Format$(p_GenRandom(lNomin), String(lPlaces, "0"))
    Case 2 ' натуральная дробь
        If (lNomin > 0) And (lDenom > 0) Then Result = Result & " " & p_GenRandom(lNomin) & "/" & p_GenRandom(lDenom)
    End Select
    
    'lblWholeDig.Visible = txtWholeDig.Visible
    txtFracType.Value = lType
    If (ActiveControl Is txtDecimalPlaces) And (txtDecimalPlaces < 1) Then txtInput.SetFocus
    txtDecimalPlaces.Visible = (txtDecimalPlaces > 0)
    If (ActiveControl Is txtSubUnit) And (txtDecimalPlaces < 1) Then txtInput.SetFocus
    If txtDecimalPlaces = 0 Then bHasSubUnit = False
    txtSubUnit.Visible = bHasSubUnit
    If (ActiveControl Is txtNominDig) And (txtNominDig < 1) Then txtInput.SetFocus
    txtNominDig = lNomin: txtNominDig.Visible = (txtNominDig > 0): lblNominDig.Visible = txtNominDig.Visible
    If (ActiveControl Is txtDenomDig) And (txtDenomDig < 1) Then txtInput.SetFocus
    txtDenomDig = lDenom: txtDenomDig.Visible = (txtDenomDig > 0): lblDenomDig.Visible = txtDenomDig.Visible
    
    p_GetNumber = Result
End Function
Private Function p_GenRandom(DigCount As Byte) As String
Dim i As Byte
Dim Result As String
    Do
    For i = 1 To DigCount: Result = Result & Chr(48 + CInt(Left$((Rnd() * 10), 1))): Next i
    Loop While Result = String(DigCount, "0")
    p_GenRandom = Result
End Function